{
  "expo": {
    "name": "Voyage AI",
    "slug": "voyage-ai",
    "version": "1.0.0",
    "orientation": "portrait", 
    "icon": "./assets/images/icon.png",
    "scheme": "voyage-ai",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAllowsArbitraryLoads": true
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser", "react-native-reanimated/plugin"],
    "experiments": {
      "typedRoutes": true
    }
  }
}